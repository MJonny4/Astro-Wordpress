---
type Props = {
    classes: string;
    styles: string;
    label: string;
    url: string;
};

import { replaceUrls } from '../utils/replaceUrls';

const { classes, styles, label, url } = Astro.props;
const href = replaceUrls(url, Astro.url.host);
---

<li class={classes} style={styles}>
    <a href={href}>{label}</a>
</li>
